{
  "name": "node-express-template",
  "version": "1.0.0",
  "description": "Node.js + ExpressJS template",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "dev": "node --env-file=.env --import=tsx --watch ./src/index.ts",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "npm run typeorm migration:run -- -d ./src/app-data-source.ts",
    "generate-entities-from-db": "typeorm-model-generator -h localhost -d postgres -u postgres -x q1w2e3r4t5 -e postgres -o ./src -s public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  }
}
